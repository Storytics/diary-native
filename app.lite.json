        "foregroundImage": "./assets/lite/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "pt.storytics.diary.lite",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~3347511713"
      },
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
